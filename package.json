{
    "name": "contentful-live-preview",
    "version": "0.1.0",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "setup": "node ./lib/contentful/setup.js",
        "format": "prettier --write ./**/*.{js,ts,tsx,css,md,json} --config ./.prettierrc.json",
        "analyze": "ANALYZE=true npm run build",
        "test": "jest --verbose --coverage",
        "test:update": "jest --updateSnapshot",
        "prepare": "husky",
        "types:update": "contentful-typescript-codegen --output src/lib/contentful/generatedTypes.d.ts",
        "models:update": "contentful space export --skip-content --content-file=./src/lib/contentful/export.json"
    },
    "dependencies": {
        "@contentful/live-preview": "^3",
        "@contentful/rich-text-react-renderer": "^15",
        "dotenv": "^16",
        "next": "^14",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@jest/globals": "^29",
        "@next/bundle-analyzer": "^14",
        "@testing-library/dom": "^10",
        "@testing-library/jest-dom": "^6",
        "@testing-library/react": "^16",
        "@testing-library/user-event": "^14",
        "@types/jest": "^29",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@types/react-test-renderer": "^18",
        "autoprefixer": "^10",
        "contentful": "^10",
        "contentful-import": "^9",
        "contentful-typescript-codegen": "^3",
        "eslint": "^8",
        "eslint-config-next": "^14",
        "eslint-config-prettier": "^9",
        "eslint-plugin-import": "^2",
        "eslint-plugin-jest": "^28",
        "eslint-plugin-jsx-a11y": "^6",
        "eslint-plugin-prettier": "^5",
        "eslint-plugin-react": "^7",
        "husky": "^9",
        "jest": "^29",
        "jest-environment-jsdom": "^29",
        "lint-staged": "^15",
        "postcss": "^8",
        "prettier": "^3",
        "tailwindcss": "^3",
        "ts-jest": "^29",
        "typescript": "^5"
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix"
    },
    "engines": {
        "node": ">=20"
    }
}
